{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["react-app", "prettier"],
    "overrides": [
    ],
    "parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
		"prettier"
    ],
    "rules": {
		"prettier/prettier": [
			"error", {
				"printWidt": 80,
				"trailingComma": "es5",
				"semi": true,
				"jsxSingleQuote": true,
				"singleQuote": true,
				"useTab": true,
				"endOfLine": "auto",
				"max-len": ["error", {"code": 80}]
			}
		]
	}
}
